<?xml version="1.0" encoding="UTF-8"?>
<metaheres:ServiceName
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:metaheres="http://www.bzh.ubo.pauline.metaheres"
    name="Heres">
  <serviceconfig
      closesOnPublicHolidays="true"
      inventoryIsPublic="true"
      hasChart="true"
      countInBusinessDays="true"
      pricingsystem="PricingFR">
    <language
        name="Français"/>
    <language
        name="English"/>
    <language
        name="Espagnol"/>
    <reservationsconfig
        minRequestAdvance="1"
        maxRequestAdvance="15">
      <statuslist>
        <status
            name="Brouillon"/>
        <status
            name="Validé"/>
        <status
            name="Fini"/>
      </statuslist>
    </reservationsconfig>
    <country
        name="France">
      <language
          name="Français"/>
      <pricingsystem
          currency="Euros"
          taxSystem="TVA_FR"
          name="PricingFR"/>
    </country>
    <country
        name="Angleterre">
      <language
          name="English"/>
      <pricingsystem
          currency="Livre Sterling"
          taxSystem="Hors-UE"
          name="SystemeAng"/>
    </country>
    <country
        name="Espagne">
      <language
          name="Espagnol"/>
      <pricingsystem
          currency="Euros"
          taxSystem="TaxUE"
          name="SystemeES"/>
    </country>
    <resourcesconfig
        hasImage="true"
        hasOneStockPerSlot="true"
        isConsumable="true"
        isCharged="true"/>
    <lotsconfig/>
  </serviceconfig>
</metaheres:ServiceName>
